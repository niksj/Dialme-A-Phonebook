<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
  <display-name>Dialme : A Phonebook</display-name>
  <welcome-file-list>
    <welcome-file>home.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>servvalidate</servlet-name>
    <servlet-class>main.ServValidate</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>servvalidate</servlet-name>
    <url-pattern>/servvalidate/*</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>servdial</servlet-name>
    <servlet-class>main.ServDial</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>servdial</servlet-name>
    <url-pattern>/servdial/*</url-pattern>
  </servlet-mapping>
</web-app>